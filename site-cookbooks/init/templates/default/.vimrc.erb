syntax enable
set vb t_vb=
set novisualbell
set number
set smartindent
set tabstop=2
set shiftwidth=2
set expandtab
set ruler
set list
set nobackup
set nowritebackup
set noswapfile
set wrap
set shiftround
set ignorecase
set smartcase
set hlsearch
set history=10000
set encoding=utf8
set fileencoding=utf8
nnoremap <C-j>  5j
nnoremap <C-k>  5k
nnoremap <C-h>  5h
nnoremap <C-l>  5l
